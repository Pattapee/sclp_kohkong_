{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty": false,
        "trailing-comma": [
            false
        ],
        "no-console": false,
        "semicolon": [
            false,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": [
            false,
            "ignore-blank-lines",
            "ignore-case",
            "locale-compare",
            "match-declaration-order",
            "shorthand-first"
        ],
        "variable-name": [
            "allow-pascal-case"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            true,
            {
                "limit": 150,
                "ignore-pattern": "^import [^,]+ from |^export | implements"
            }
        ]
    },
    "rulesDirectory": []
}